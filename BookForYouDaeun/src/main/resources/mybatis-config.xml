<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.bookforyou.bk4u.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.bookforyou.bk4u.book.model.vo.Book" alias="Book"/>
		<typeAlias type="com.bookforyou.bk4u.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.bookforyou.bk4u.booklist.model.vo.Booklist" alias="Booklist"/>
		<typeAlias type="com.bookforyou.bk4u.subscription.model.vo.Subscription" alias="Subscription"/>
		<typeAlias type="com.bookforyou.bk4u.reply.model.vo.Reply" alias="Reply"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="/mappers/member-mapper.xml"/>
		<mapper resource="/mappers/board-mapper.xml"/>
		<mapper resource="/mappers/booklist-mapper.xml"/>
		<mapper resource="/mappers/subscription-mapper.xml"/>
	</mappers>
	
</configuration>
 